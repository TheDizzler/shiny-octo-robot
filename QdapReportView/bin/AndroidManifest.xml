<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vdindustries.qdapreportview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="vdindustries.qdapreportview.TradeListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="vdindustries.qdapreportview.TradeDetailActivity"
            android:label="@string/title_trade_detail"
            android:parentActivityName="vdindustries.qdapreportview.TradeListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="vdindustries.qdapreportview.TradeListActivity" />
        </activity>
    </application>

</manifest>
